.Land.blue .cardtitlebar, .Land.blue .cardtypebar { background-color: $Land_Blue_Bar; }
.Land.blue .cardtext, .Land.blue .pinline_ptbox { background-color: $Land_Blue_Text; }

.Land.black .cardtitlebar, .Land.black .cardtypebar { background-color: $Land_Black_Bar; }
.Land.black .cardtext, .Land.black .pinline_ptbox { background-color: $Land_Black_Text; }

.Land.red .cardtitlebar, .Land.red .cardtypebar { background-color: $Land_Red_Bar; }
