.common, .token, .basic {  color: #fff;
  background-color: #000;}
.uncommon {
  background: #ccc;
  @include gradient_diag_highlight(#999, #ccd3d6);
 }
.rare   {  color: #000;
  background: #e0d060;
  @include gradient_diag_highlight(#998044, #e0d060);
}
.mythic { color: #fff;
  background: #933;
  @include gradient_diag_highlight(#a43, #e86);
}


