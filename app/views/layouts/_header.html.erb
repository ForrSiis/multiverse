<header>
  <%= link_to logo, root_path %>
  <nav class="round">
    <ul>
      <li class="first"><%= link_to "Home", root_path %></li>
      <!-- li><!--%= link_to "Contact", contact_path % --></li -->
      <!-- li><!-- %= link_to "Help", help_path % --></li -->
      <% if signed_in? %>
      <li class="subsequent">Signed in as <%= link_to current_user.name, current_user %></li>
      <li class="subsequent"><%= link_to "Sign out", signout_path, :method => :delete %></li>
      <% else %>
      <li class="subsequent"><%= link_to "Sign in", signin_path %></li>
      <% end %>
    </ul>
  </nav>
</header>
